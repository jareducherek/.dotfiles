export XDG_CONFIG_HOME=$HOME/.config
VIM="nvim"

export CC="clang-12"
export CXX="clang++-12"
export PYTHONBREAKPOINT="pudb.set_trace"
export GOPATH=$HOME/go
export GIT_EDITOR=$VIM
export DENO_INSTALL="$HOME/.deno"

# Where should I put you?
bindkey -s ^f "tmux-sessionizer\n"

alias mongod='brew services run mongodb-community'
alias mongod-status='brew services list'
alias mongod-stop='brew services stop mongodb-community'

# >>> conda initialize >>>
# !! Contents within this block are managed by 'conda init' !!
__conda_setup="$('/opt/anaconda3/bin/conda' 'shell.bash' 'hook' 2> /dev/null)"
if [ $? -eq 0 ]; then
    eval "$__conda_setup"
else
    if [ -f "/opt/anaconda3/etc/profile.d/conda.sh" ]; then
        . "/opt/anaconda3/etc/profile.d/conda.sh"
    else
        export PATH="/opt/anaconda3/bin:$PATH"
    fi
fi
unset __conda_setup
#<<< conda initialize <<<
jupyter_list (){
    jupyter kernelspec list
}
jupyter_add (){
    python -m ipykernel install --user --name $1
}
jupyter_remove (){
    jupyter kernelspec uninstall $1
}

[[ -s $HOME/.nvm/nvm.sh ]] && . $HOME/.nvm/nvm.sh  # This loads NVM
alias vim='nvim'
[ -s "$NVM_DIR/bash_completion" ] && \. "$NVM_DIR/bash_completion"  # This loads nvm bash_completion

export JAVA_HOME=/usr/local/opt/openjdk@8/libexec/openjdk.jdk/Contents/Home
export N_PREFIX="$HOME/n"; [[ :$PATH: == *":$N_PREFIX/bin:"* ]] || PATH+=":$N_PREFIX/bin"  # Added by n-install (see http://git.io/n-install-repo).
export PATH="/Library/Developer/CommandLineTools/usr/bin/:$PATH"
export SDKROOT="`xcrun --show-sdk-path`"
export CPLUS_INCLUDE_PATH="/usr/local/opt/libomp/include/:$CPLUS_INCLUDE_PATH"


catr() {
    tail -n "+$1" $3 | head -n "$(($2 - $1 + 1))"
}

validateYaml() {
    python -c 'import yaml,sys;yaml.safe_load(sys.stdin)' < $1
}

goWork() {
    cp ~/.npm_work_rc ~/.npmrc
}

goPersonal() {
    cp ~/.npm_personal_rc ~/.npmrc
}

cat1Line() {
    cat $1 | tr -d "\n"
}

eslintify() {
    cat $1 > /tmp/file_to_eslint
    npx eslint
}
